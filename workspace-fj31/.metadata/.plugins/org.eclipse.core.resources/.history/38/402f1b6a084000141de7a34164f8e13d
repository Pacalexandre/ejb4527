	public static void main(String[] args) {
		HttpClient http = new DefaultHttpClient();
		HttpPost post = new HttpPost("http://localhost:8080/fj31-loja-rest/loja/livros");
		post.addHeader("content-type","application/json");
		
		StringEntity
	}
}
